				"source": [
					"%%sql\r\n",
					"CREATE OR REPLACE TEMP VIEW indices_juros AS\r\n",
					"select\r\n",
					"    CAST(replace(p.newpk, \"/\", \"-\") as string) as id\r\n",
					"    -- , 'NENHUM' as avaliacao_risco --removido, carga inicial da processo j√° o faz\r\n",
					"    , CASE \r\n",
					"        WHEN upper(ti.nom_tipo_indice_atualizacao) IN ('SELIC', 'INPC') THEN True\r\n",
					"        WHEN upper(ti.nom_tipo_indice_atualizacao) IN ('TR') THEN False\r\n",
					"        ELSE null\r\n",
					"    END as indice_correcao_monetaria_padrao\r\n",
					"    , CASE \r\n",
					"        WHEN upper(ti.nom_tipo_indice_atualizacao) IN ('SELIC', 'INPC') THEN null\r\n",
					"        WHEN upper(ti.nom_tipo_indice_atualizacao) IN ('TR') THEN 'TR'\r\n",
					"        ELSE 'ERRO'\r\n",
					"    END as index_custom_name\r\n",
					"    , True as taxa_juros_padrao\r\n",
					"    , null index_custom_fees\r\n",
					"from silver.processo p\r\n",
					"INNER JOIN silver.tipo_indice_atualizacao ti on ti.cod_tipo_indice_atualizacao = p.cod_tipo_indice_atualizacao\r\n",
					"WHERE upper(ti.nom_tipo_indice_atualizacao) IN ('TR') "
				],
				"execution_count": 15
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"microsoft": {
						"language": "sparksql"
					},
					"collapsed": false
				},
				"source": [
					"%%sql\r\n",
					"\r\n",
					"----Verificar se teve erros\r\n",
					"SELECT count(*)\r\n",
					"FROM indices_juros\r\n",
					"WHERE\r\n",
					"(\r\n",
					"    indice_correcao_monetaria_padrao IS NULL\r\n",
					"    OR\r\n",
					"    index_custom_name = \"ERRO\"\r\n",
					")"
				],
