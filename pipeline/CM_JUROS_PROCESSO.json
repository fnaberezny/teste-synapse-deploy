					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "NonQuery",
							"text": {
								"value": "update fi\n   set prov_cp_cm_final = cmjp.prov_cp_cm_final\n     , prov_cp_juros_final = cmjp.prov_cp_juros_final\n     , prov_lp_cm_final = cmjp.prov_lp_cm_final\n     , prov_lp_juros_final = cmjp.prov_lp_juros_final\n     , possivel_op_final = cmjp.possivel_op_final\n     , possivel_cm_final = cmjp.possivel_cm_final\n     , possivel_juros_final = cmjp.possivel_juros_final\n     , possivel_fi_final = cmjp.possivel_fi_final\n     , possivel_op_fi_final = cmjp.possivel_op_e_fi_final\n     , remoto_op_final = cmjp.remoto_op_final\n     , remoto_cm_final = cmjp.remoto_cm_final\n     , remoto_juros_final = cmjp.remoto_juros_final\n     , remoto_fi_final = cmjp.remoto_fi_final\n     , remoto_op_fi_final = cmjp.remoto_op_e_fi_final\n     , updated_at = format(getdate() at time zone 'UTC' at time zone 'E. South America Standard Time', 'yyyy-MM-dd HH:mm:ss')\n  from fotografia_item fi\n inner join carga_inicial_cm_juros_processo cmjp\n    on cmjp.sequencial = fi.sequencial\n where fi.cod_calendario_monetario = @{variables('cod_calendario_monetario_anterior')}",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "get-cod_calendario_monetario_anterior",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "select cast(max(cod_calendario_monetario) as varchar(4)) cod_calendario_monetario_anterior\n  from Silver.calendario_monetario\n where cod_calendario_monetario < (select cod_calendario_monetario\n                                     from Silver.calendario_monetario\n                                    where sts_ativo = 1\n                                  )",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
