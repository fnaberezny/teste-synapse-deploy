					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "Pool Dedicated",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "NonQuery",
							"text": {
								"value": "insert\n  into rl3.processos_pendentes\n     ( num_processo\n     , cod_empresa\n     , sig_escritorio_contratado\n     , cod_divisao_processo\n     , pipeline_id\n     , table_changed\n     , tipo_processo\n     )\nselect distinct p.num_processo\n     , p.cod_empresa\n     , sig_escritorio_contratado\n     , cod_divisao_processo\n     , '@{pipeline().RunId}'\n     , 'CARGA INICIAL MANUAL'\n     , 'N'\n  from carga_inicial_cm_juros_processo cmjp\n inner join Silver.processo p\n    on p.num_processo = cmjp.sequencial",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
